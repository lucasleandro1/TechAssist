<main>
<h1>TechAssist</h1>
<%= form_with url: search_clients_path, method: :get, local: true do %>
  <div class="row">
    <div class="col-sm">
      <div class="form-group">
        <%= label_tag :q_cpf_cont, "Cliente" %>
        <%= text_field_tag :q_cpf_cont, params[:q_cpf_cont], class: "form-control", placeholder: "Digite o CPF" %>
      </div>
    </div>
    <div class="col-sm">
      <%= submit_tag "Pesquisar", class: "btn btn-danger", style: "margin-top: 31px;" %>
    </div>
  </div>
<% end %>
<%= form_with url: search_mobile_device_path, method: :get, local: true do %>
  <div class="row">
    <div class="col-sm">
      <div class="form-group">
        <%= label_tag :q_imei_cont, "Device" %>
        <%= text_field_tag :q_imei_cont, params[:q_imei_cont], class: "form-control", placeholder: "Digite o IMEI" %>
      </div>
    </div>
    <div class="col-sm">
      <%= submit_tag "Pesquisar", class: "btn btn-danger", style: "margin-top: 31px;" %>
    </div>
  </div>
<% end %>



</main>
